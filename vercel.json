{
  "version": 2,
  "builds": [
    {
      "src": "Dockerfile",
      "use": "@vercel/docker"
    }
  ],
  "env": {
    "SPRING_DATASOURCE_URL": "@jdbc:mysql://localhost:3306/fir",
    "SPRING_DATASOURCE_USERNAME": "@root",
    "SPRING_DATASOURCE_PASSWORD": "@maha"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Online-FIR-System.jar"
    }
  ]
}
